    "from sklearn import neighbors, datasets\n",
    "iris = datasets.load_iris()\n",
    "X = iris.data[:, :2]\n",
    "y = iris.target\n",
    "clf = neighbors.KNeighborsClassifier(3)\n",
    "clf.fit(X, y)\n",
    "clf.n_neighbors"
   ]
